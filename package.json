{
  "name": "meet",
  "version": "1.0.0",
  "description": "A backend meeting app",
  "main": "index.js",
  "scripts": {
    "build": "npm run clear && mkdir dist && babel src -d dist",
    "start": "NODE_ENV=production && node ./bin/prod",
    "start-dev": "NODE_ENV=development && nodemon --legacy-watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "node bin/migrate",
    "unmigrate": "node bin/unMigrate"
  },
  "author": "Opeoluwa Iyi-Kuyoro",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@respondex/core": "^0.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3"
  },
  "nodemonConfig": {
    "legacyWatch": true,
    "delay": "1500",
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js",
      ".git"
    ],
    "watch": [
      "src"
    ],
    "exec": "node ./bin/dev",
    "ext": "js"
  }
}
